{
  "name": "web-2d-game-engine",
  "version": "0.4.1",
  "description": "A lightweight 2D game engine for web browsers with ECS architecture",
  "main": "dist/gameengine.umd.js",
  "module": "dist/gameengine.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "game-engine",
    "2d",
    "webgl",
    "canvas",
    "ecs",
    "physics",
    "typescript"
  ],
  "author": "GameEngine Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lemur-bookstores/web-2d-game-engine.git"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.7",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "9.33.0",
    "jsdom": "26.1.0",
    "terser": "5.43.1",
    "typescript": "5.9.2",
    "vite": "7.1.3",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@types/uuid": "10.0.0",
    "box2d-wasm": "7.0.0",
    "gl-matrix": "3.4.4",
    "uuid": "11.1.0"
  }
}
